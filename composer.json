{
    "name": "izaghar/xivapi-laravel",
    "description": "Laravel integration for XIVAPI PHP client",
    "keywords": ["xivapi", "ffxiv", "final-fantasy", "laravel", "api-client"],
    "homepage": "https://github.com/izaghar/xivapi-laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "XivApi\\Laravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "izaghar"
        }
    ],
    "require": {
        "php": "^8.3",
        "izaghar/xivapi-php": "^2.0",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "XivApi\\Laravel\\XivApiServiceProvider"
            ],
            "aliases": {
                "XivApi": "XivApi\\Laravel\\Facades\\XivApi"
            }
        }
    }
}
